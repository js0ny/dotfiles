// ~/Library/Keybindings/DefaultKeyBinding.dict
{
    // Colemak Vim Style Arrows
    "^h" = "moveBackward:";
    "^n" = "moveDown:";
    "^e" = "moveUp:";
    "^i" = "moveForward:";
    // Colemak Vim Style Home/End, Insert & Append
    "^l" = "moveToBeginningOfLine:";
    "^a" = "moveToEndOfLine:";
}
