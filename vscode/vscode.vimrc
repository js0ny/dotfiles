"vim.vimrc.path": "$HOME/.vscode.vimrc", // from vscode settings.json

" Colemak 方向键映射 (hnei）
nnoremap k n
nnoremap K N
nnoremap j e
vnoremap j e
nnoremap l i
nnoremap N J
nnoremap E K
nnoremap I L
nnoremap J E
vnoremap J E
nnoremap L I
nnoremap k n
nnoremap K N
nnoremap j e
vnoremap j e
nnoremap l i
nnoremap N J
nnoremap E K
nnoremap I L
nnoremap J E
vnoremap J E
nnoremap L I
" 方向键映射
noremap n <Down>     " 向下
noremap e <Up>       " 向上
noremap i <Right>    " 向右
